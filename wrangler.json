{
	"$schema": "node_modules/wrangler/config-schema.json",
	"assets": {
		"directory": "out",
		"not_found_handling": "404-page"
	},
	"build": {
		"command": "pnpm run build"
	},
	"compatibility_date": "2025-12-01",
	"name": "cg-hermes",
	"observability": {
		"enabled": false,
		"head_sampling_rate": 1,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true
		},
		"traces": {
			"enabled": false,
			"head_sampling_rate": 1,
			"persist": true
		}
	},
	"preview_urls": true,
	"route": {
		"custom_domain": true,
		"pattern": "carbophile.org"
	},
	"workers_dev": false
}
