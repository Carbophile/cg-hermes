{
	"$schema": "node_modules/wrangler/config-schema.json",
	"assets": {
		"directory": "out",
		"not_found_handling": "404-page"
	},
	"build": {
		"command": "pnpm run build"
	},
	"compatibility_date": "2025-12-01",
	"env": {
		"production": {
			"vars": {
				"NEXT_PUBLIC_IMAGE_OPTIMIZATION": true
			}
		}
	},
	"name": "cg-hermes",
	"preview_urls": true,
	"route": {
		"custom_domain": true,
		"pattern": "carbophile.org"
	},
	"workers_dev": false
}
